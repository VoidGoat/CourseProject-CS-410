{
  "manifest_version": 2,
  "name": "Flexible Find",
  "version": "1.0",
  "description": "Searches web pages in a more flexible way",
  "icons": {
    "48": "icons/icon-48.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icons/icon-48.png",
    "default_title": "Flexible Find",
    "default_popup": "popup/search_bar.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "content_scripts/content.css"
      ],
      "js": [
        "content_scripts/fuzzyset.js",
        "content_scripts/find.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "content_scripts/wordnet-js/lib/wordnet-file.js",
    "content_scripts/wordnet-js/lib/data-file.js",
    "content_scripts/wordnet-js/lib/index-file.js",
    "content_scripts/wordnet-js/lib/wordnet.js"
  ]
}